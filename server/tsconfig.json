{
  "compilerOptions": {
    "target": "es2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": [
      "es2020",
      "DOM"
    ],
    "baseUrl": "./",
    "outDir": "./dist/tsc",
    "noUnusedLocals": false,
    "sourceMap": true,
    "noUnusedParameters": false,
    "forceConsistentCasingInFileNames": false,
    "incremental": true,
    "tsBuildInfoFile": "./dist/cache/tsc.json",
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
  }
}